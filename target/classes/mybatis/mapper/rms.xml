<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="rms">
	<!-- 푸시알람을 보내야 하는 알람의 목록 -->
	<select id="getRmsData" resultType="rms">
		SELECT *
		FROM tb_rms
	</select>
	
	<insert id="setHT01RmsInsert" parameterType="HT01Rms">
		INSERT INTO tb_rms_data1
		VALUES
		(#{equip_code}, #{lotno}, 
		#{qf1_temp}, #{qf1_time}, #{qf2_temp}, #{qf2_time})
	</insert>
</mapper>